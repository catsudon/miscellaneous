   "source": [
    "from PIL import Image\n",
    "import numpy as np\n",
    "\n",
    "w, h = 1024, 512\n",
    "data = np.zeros((h, w, 3), dtype=np.uint8)\n",
    "data[0:h , 0:w] = [255, 161, 239]\n",
    "img = Image.fromarray(data, 'RGB')\n",
    "img.save('./img/pink_bg.png')\n",
    "img.show()"
   ]
